{
  "name": "@hero/HeroTravelerMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "concurrently -r 'npm run share:watch' 'react-native run-ios'",
    "lint": "eslint --quiet App/**/*.js",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && yarn install",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "dev-test-link-reset": "xcrun simctl openurl booted \"com.herotraveler.herotraveler-beta://resetpassword/a6671bb4-73df-4110-8a7b-c1f1dd579707\"",
    "dev-test-link-email": "xcrun simctl openurl booted \"com.herotraveler.herotraveler-beta://emailverify/dc24a446-3a76-477a-8f17-255dfbaf0892\"",
    "share": "rimraf App/Shared && mkdir App/Shared && cpx \"../shared/**/*.{ts,tsx,js,jsx,json,ttf,png,svg}\" App/Shared",
    "share:watch": "rimraf App/Shared && mkdir App/Shared && cpx \"../shared/**/*.{ts,tsx,js,jsx,json,ttf,png,svg}\" App/Shared --watch"
  },
  "dependencies": {
    "algoliasearch": "^3.22.1",
    "algoliasearch-helper": "^2.20.1",
    "apisauce": "^0.10.0",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "cpx": "^1.5.0",
    "draft-js": "0.10",
    "eslint-plugin-react-native": "^2.3.2",
    "format-json": "^1.0.3",
    "immutable": "3.7.6",
    "keymirror": "0.1.1",
    "lodash": "^4.17.2",
    "moment": "^2.18.1",
    "normalizr": "^3.2.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.5.10",
    "querystringify": "0.0.4",
    "ramda": "^0.23.0",
    "react": "16.2.0",
    "react-mixin": "^3.0.5",
    "react-native": "0.53",
    "react-native-animatable": "^1.1.0",
    "react-native-autogrow-textinput": "5.1.1",
    "react-native-camera": "0.6",
    "react-native-device-info": "^0.10.0",
    "react-native-draftjs-render": "2.4.0",
    "react-native-drawer": "^2.3.0",
    "react-native-fbsdk": "^0.6.0",
    "react-native-fetch-blob": "^0.10.5",
    "react-native-global-props": "^1.1.1",
    "react-native-google-places": "^2.4.2",
    "react-native-hockeyapp": "^0.5.1",
    "react-native-htmlview": "^0.9.0",
    "react-native-image-picker": "^0.26.2",
    "react-native-img-cache": "^1.4.0",
    "react-native-keyboard-spacer": "^0.3.1",
    "react-native-keyboard-tracking-view": "5.1.3",
    "react-native-maps": "~0.13.0",
    "react-native-media-meta": "^0.0.10",
    "react-native-push-notification": "^2.2.1",
    "react-native-router-flux": "https://github.com/rwoody/react-native-router-flux",
    "react-native-splash-screen": "^2.0.0",
    "react-native-vector-icons": "^4.0.0",
    "react-native-version-number": "^0.1.2",
    "react-native-webview-bridge-updated": "https://github.com/rwoody/react-native-webview-bridge#container-style",
    "react-native-zss-rich-text-editor": "https://github.com/rwoody/react-native-zss-rich-text-editor#insert-video",
    "react-redux": "^5.0.2",
    "react-timer-mixin": "^0.13.3",
    "recompose": "^0.23.5",
    "redux": "^3.6.0",
    "redux-form": "7.2.3",
    "redux-logger": "^2.8.2",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.14.3",
    "reduxsauce": "0.4.1",
    "rimraf": "^2.6.2",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.18.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.13.1",
    "ignite-animatable": "^0.3.1",
    "ignite-ir-boilerplate-2016": "^0.2.0",
    "ignite-push-notification": "^0.0.2",
    "ignite-vector-icons": "^0.2.1",
    "mockery": "^2.0.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.4.0",
    "react-native-mock": "^0.3.1",
    "reactotron-apisauce": "^1.7.0",
    "reactotron-react-native": "^1.7.0",
    "reactotron-redux": "^1.7.0",
    "reactotron-redux-saga": "^1.7.0",
    "snazzy": "^6.0.0",
    "standard": "8.6.0"
  },
  "ava": {
    "files": [
      "App/**/*.test.js",
      "Tests/**/*.js",
      "!Tests/Setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./Tests/Setup"
    ],
    "babel": "inherit"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "if [ -d 'ignite-base' ]; then cd ignite-base; fi; npm run lint"
    }
  },
  "rnpm": {
    "assets": [
      "./App/Fonts"
    ]
  }
}
