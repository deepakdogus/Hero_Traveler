  "scripts": {
    "start": "PORT=3001 NODE_ENV=development react-scripts start",
    "build": "react-scripts build",
    "postbuild": "node check-service-worker.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "share": "rm -rf src/App/Shared && mkdir src/App/Shared && cpx \"../shared/**/*.{ts,tsx,js,jsx,json,ttf,png,svg,jpg,css,ttf}\" src/App/Shared && mkdir src/App/Shared/Web && cpx \"../sharedWeb/**/*.{ts,tsx,js,jsx,json,ttf,png,svg,jpg,css,ttf}\" src/App/Shared/Web",
    "watchShared": "rimraf src/App/Shared && mkdir src/App/Shared && cpx \"../shared/**/*.{ts,tsx,js,jsx,json,ttf,png,svg,jpg,css,ttf}\" src/App/Shared --watch",
    "watchSharedWeb": "rimraf src/App/Shared/Web && mkdir src/App/Shared/Web && cpx \"../sharedWeb/**/*.{ts,tsx,js,jsx,json,ttf,png,svg,jpg,css,ttf}\" src/App/Shared/Web --watch",
    "share:watch": "concurrently --kill-others \"npm run watchShared\" \"npm run watchSharedWeb\""
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "cpx": "^1.5.0",
    "reactotron-apisauce": "^1.12.2",
    "reactotron-react-js": "^1.12.2",
    "reactotron-redux": "^1.12.2",
