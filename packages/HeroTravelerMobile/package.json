{
  "name": "HeroTravelerMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "concurrently -r 'npm run share:watch' 'react-native run-ios'",
    "share": "rimraf App/Shared && mkdir App/Shared && cpx \"../shared/**/*.{ts,tsx,js,jsx,json,ttf,png,svg}\" App/Shared",
    "share:watch": "rimraf App/Shared && mkdir App/Shared && cpx \"../shared/**/*.{ts,tsx,js,jsx,json,ttf,png,svg}\" App/Shared --watch",
    "test": "jest"
  },
  "dependencies": {
    "apisauce": "^1.0.2",
    "lodash": "^4.17.11",
    "normalizr": "^3.3.0",
    "react": "16.8.3",
    "react-native": "0.59.0",
    "react-native-router-flux": "^4.0.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.1.0",
    "redux-saga": "^1.0.2",
    "reduxsauce": "^1.0.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "babel-jest": "^24.5.0",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "jest": "^24.5.0",
    "metro-react-native-babel-preset": "^0.53.0",
    "react-test-renderer": "16.8.3",
    "rimraf": "^2.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
